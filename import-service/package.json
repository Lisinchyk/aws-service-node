{
  "name": "import-service",
  "version": "1.0.0",
  "description": "aws-nodejs-digital-shop",
  "main": "serverless.ts",
  "scripts": {
    "deploy:dev": "sls deploy --stage dev",
    "deploy:prod": "sls deploy --stage prod",
    "sls:invoke": "serverless invoke local --function",
    "remove:dev-stack": "sls remove --stage dev --region us-east-1",
    "remove:prod-stack": "sls remove --stage prod --region us-east-1",
    "test": "jest --config=jest.config.js",
    "test:coverage": "npm run test --coverage"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.288.0",
    "@middy/core": "^3.4.0",
    "@middy/http-json-body-parser": "^3.4.0",
    "aws-sdk": "^2.1332.0",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/jest": "^29.4.0",
    "@types/node": "^14.14.25",
    "aws-sdk-mock": "^5.8.0",
    "jest": "^29.4.3",
    "json-schema-to-ts": "^1.5.0",
    "lambda-tester": "^4.0.1",
    "serverless": "^3.0.0",
    "serverless-webpack": "^5.11.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^8.4.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.1.3",
    "webpack": "^4.29.0",
    "webpack-node-externals": "^3.0.0"
  },
  "author": "Anatolii L",
  "license": "MIT"
}
